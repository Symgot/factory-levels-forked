{
  "name": "@factorify/api",
  "version": "1.0.0",
  "description": "Cross-Repository API for Factorio Mod Analysis - ML-based pattern recognition, performance optimization, and security scanning",
  "main": "backend_api/server.js",
  "scripts": {
    "start": "node backend_api/server.js",
    "dev": "nodemon backend_api/server.js",
    "test": "jest",
    "lint": "eslint .",
    "format": "prettier --write '**/*.{js,json,md,yml}'",
    "build": "npm run lint && npm run test",
    "migrate": "node scripts/migrate.js",
    "worker": "node distributed_orchestration/worker.js",
    "supervisor": "node distributed_orchestration/supervisor.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Symgot/Factorify.git"
  },
  "keywords": [
    "factorio",
    "mod",
    "analysis",
    "ml",
    "pattern-recognition",
    "performance",
    "obfuscation",
    "security",
    "api",
    "cross-repository"
  ],
  "author": "Symgot",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Symgot/Factorify/issues"
  },
  "homepage": "https://github.com/Symgot/Factorify#readme",
  "dependencies": {
    "@octokit/app": "^14.0.0",
    "@octokit/rest": "^20.0.0",
    "axios": "^1.6.0",
    "express": "^4.18.0",
    "graphql": "^16.8.0",
    "express-graphql": "^0.12.0",
    "jsonwebtoken": "^9.0.0",
    "p-queue": "^7.4.0",
    "p-retry": "^6.1.0",
    "winston": "^3.11.0",
    "redis": "^4.6.0"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.0",
    "prettier": "^3.1.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
